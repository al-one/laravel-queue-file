{
    "name": "al-one/laravel-queue-file",
    "description": "File based driver for Laravel Queue",
    "keywords": ["laravel", "illuminate", "queue", "file-based", "laravel-queues"],
    "authors": [
        {
            "name": "Alone",
            "email": "hi@anlo.ng"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/contracts": "^5.7 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/queue": "^5.7 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/support": "^5.7 || ^6.0 || ^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Alone\\LaravelQueueFile\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alone\\LaravelQueueFile\\ServiceProvider"
            ]
        }
    },
    "license": "MIT"
}
